platform :ios, '11.0'


target 'AlefSwift' do
  use_frameworks!
  inhibit_all_warnings!
  
  # ВАЖНО! Когда добавляете под или раскомментируете его
  # всегда добавляйте к нему номер версии, которая установилась в первый раз
  # Например: pod 'MBProgressHUD', '1.2.0'
  # Посмотреть эту ферсию можно в Podfile.lock
  
	pod 'MBProgressHUD', '1.2.0'; # индикатор загрузки
	pod 'SideMenuSwift', '2.0.5'
	pod 'ActionSheetPicker-3.0', '2.5.0'
  # pod 'UIView+Shake' # вибрация элементов (для валидации)
  # pod 'INTULocationManager' # класс для работы с GPS
	pod 'SDWebImage', '5.8.4' # Загрузка картинок из интернета
  # pod 'IDMPhotoBrowser' # галерея фотографий на отдельном экране
  # pod 'CircleProgressBar' # симпатичный индикатор загрузки (круг)
  # pod 'VoxImplantSDK' # Офигительная VOIP-телефония
  pod 'SCLAlertView', '0.8' # Кастомизируемые алерты
    
end



target 'AlefSwiftLayout' do
	use_frameworks!
	inhibit_all_warnings!
	
	# ВАЖНО! Когда добавляете под или раскомментируете его
	# всегда добавляйте к нему номер версии, которая установилась в первый раз
	# Например: pod 'MBProgressHUD', '1.2.0'
	# Посмотреть эту ферсию можно в Podfile.lock
	
	pod 'MBProgressHUD', '1.2.0'; # индикатор загрузки
	pod 'SideMenuSwift', '2.0.5'
	pod 'ActionSheetPicker-3.0', '2.5.0'
	# pod 'UIView+Shake' # вибрация элементов (для валидации)
	# pod 'INTULocationManager' # класс для работы с GPS
	pod 'SDWebImage' # Загрузка картинок из интернета
	# pod 'IDMPhotoBrowser' # галерея фотографий на отдельном экране
	# pod 'CircleProgressBar' # симпатичный индикатор загрузки (круг)
	# pod 'VoxImplantSDK' # Офигительная VOIP-телефония
	pod 'SCLAlertView', '0.8' # Кастомизируемые алерты
	
	
	target 'AlefSwiftLayoutUITests' do
		
	end
	
end
